<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="195.0" prefWidth="306.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ejercicios.gui.ContAdivinaNumeroConFor">
   <children>
      <HBox alignment="CENTER" prefHeight="1.0" prefWidth="200.0">
         <children>
            <Button mnemonicParsing="false" onAction="#probarNumero" prefHeight="26.0" prefWidth="100.0" text="Probar" />
            <TextField fx:id="tEscribir" />
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
      <HBox alignment="CENTER" prefHeight="0.0" prefWidth="200.0">
         <children>
            <Label text="Numero de intentos: " />
            <Label fx:id="intentos" text="10" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
      <HBox alignment="BOTTOM_CENTER" prefHeight="76.0" prefWidth="306.0">
         <children>
            <TextArea fx:id="tResult" editable="false" prefHeight="80.0" prefWidth="296.0" wrapText="true" />
         </children>
      </HBox>
   </children>
</VBox>
